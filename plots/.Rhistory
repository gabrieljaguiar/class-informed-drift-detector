library(ggplot2)
library(reshape2)
library(dplyr)
library(ggpubr)
n_classes = c(3,5,10,15,20)
for (n_c in n_classes){
ciddm <- read.csv(paste0("../output/alerts_prune_growth_new_branch_local_sudden_",n_c,".csv"))
ht_acc <- read.csv(paste0("../output/HT_ADWIN_prune_growth_new_branch_local_sudden_",n_c,".csv"))
ht_adwin <- read.csv(paste0("../output/drift_alerts_HT_ADWIN_prune_growth_new_branch_local_sudden_",n_c,".csv"))
nb_adwin <- read.csv(paste0("../output/drift_alerts_NB_ADWIN_prune_growth_new_branch_local_sudden_",n_c,".csv"))
columns <- c("idx", "accuracy", paste0("class_", n_c-1))
acc_df <- ht_acc[, columns]
acc_melt <- melt(acc_df, id.vars="idx")
g_ciddm <- ggplot(acc_melt, aes(x=idx, y=value)) +
geom_line(aes(color=variable)) +
scale_x_continuous(labels=scales::comma) +
geom_vline(xintercept = c(100000, 200000, 300000),
color="black", alpha=0.8, linetype=3, linewidth=1) +
geom_vline(xintercept = ciddm$idx,
color="red", alpha=0.8, linetype=3, linewidth=1) +
ggtitle("CIDDM") +
theme_bw()
g_adwin <- ggplot(acc_melt, aes(x=idx, y=value)) +
geom_line(aes(color=variable)) +
scale_x_continuous(labels=scales::comma) +
geom_vline(xintercept = c(100000, 200000, 300000),
color="black", alpha=0.8, linetype=3, linewidth=1) +
geom_vline(xintercept = ht_adwin$idx,
color="red", alpha=0.8, linetype=3, linewidth=1) +
ggtitle("ADWIN") +
theme_bw()
figure <- ggarrange(g_ciddm, g_adwin,
labels = c("", ""),
ncol = 2, nrow = 1)
ggsave(paste0("prune_growth_new_branch_local_sudden_",n_c,".pdf"),width=16.77, height=3)
}
